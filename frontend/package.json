{
  "name": "geoblog-rf",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "node build-production.js",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest --config vitest.config.cjs",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/database/config/database.config.ts src/database/migrations/NewMigration",
    "migration:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/database/config/database.config.ts",
    "db:check": "node --require ts-node/register --require tsconfig-paths/register src/database/utils/checkConnection.ts",
    "test:db": "ts-node src/database/tests/marker-user.test.ts",
    "clean": "rm -rf dist node_modules/.vite",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix"
  },
